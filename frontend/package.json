{
    "name": "professional-ecommerce-frontend",
    "version": "1.0.0",
    "description": "Professional e-commerce frontend built with React and modern best practices",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives",
        "lint:fix": "eslint . --ext js,jsx --fix",
        "test": "vitest run",
        "test:watch": "vitest",
        "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
        "clean": "rm -rf dist node_modules/.vite",
        "analyze": "vite-bundle-analyzer"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@hookform/resolvers": "^5.2.2",
        "@reduxjs/toolkit": "^2.11.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.71.1",
        "react-hot-toast": "^2.6.0",
        "react-redux": "^9.2.0",
        "react-router-dom": "^6.30.3",
        "yup": "^1.7.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@testing-library/user-event": "^14.6.1",
        "@vitejs/plugin-react": "^4.3.3",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.14.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.14",
        "jsdom": "^28.1.0",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.14",
        "vite": "^5.4.10",
        "vitest": "^4.0.18"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "react",
        "ecommerce",
        "frontend",
        "vite",
        "tailwindcss",
        "professional"
    ],
    "author": "Professional E-Commerce Team",
    "license": "MIT"
}
