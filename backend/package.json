{
    "name": "backend",
    "version": "1.0.0",
    "description": "Backend API for Enterprise E-commerce",
    "type": "module",
    "main": "src/server.js",
    "scripts": {
        "dev": "nodemon src/server.js",
        "start": "node src/server.js",
        "test": "node --experimental-vm-modules ../node_modules/jest/bin/jest.js --coverage --passWithNoTests",
        "test:watch": "node --experimental-vm-modules ../node_modules/jest/bin/jest.js --watch",
        "lint": "eslint . --ext .js",
        "lint:fix": "eslint . --ext .js --fix",
        "format": "prettier --write \"src/**/*.js\"",
        "seed": "node scripts/seed.js",
        "seed:minimal": "node scripts/seed.js -- --minimal",
        "seed:development": "node scripts/seed.js -- --development",
        "seed:production": "node scripts/seed.js -- --production",
        "seed:products": "node scripts/seed.js -- --skip-users",
        "queue:start": "node src/queues/worker.js"
    },
    "keywords": [
        "ecommerce",
        "api",
        "express",
        "mongodb",
        "javascript"
    ],
    "author": "Enterprise Team",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "bull": "^4.12.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.4.1",
        "express-validator": "^7.2.0",
        "helmet": "^8.0.0",
        "hpp": "^0.2.3",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.8.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.15",
        "slugify": "^1.6.6",
        "stripe": "^17.2.1",
        "uuid": "^11.0.2",
        "winston": "^3.15.0"
    },
    "devDependencies": {
        "eslint": "^9.14.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "supertest": "^7.2.2"
    },
    "jest": {
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).cjs"
        ]
    }
}
